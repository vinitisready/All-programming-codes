class Solution {
    int floorSqrt(int n) {
        int left=1;
        int right=n;
        int ans=1;
        for(int i=1;i<n;i++){
            int mid=(left+right)/2;
            if(mid*mid<=n){
                // ans=mid;
                left=mid+1;
            }
            else{
                right=mid-1;
            }
        }

        return right;
    }
}
